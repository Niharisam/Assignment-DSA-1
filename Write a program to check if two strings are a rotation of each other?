def areRotations(string1,string2):
    size1=len(string1)
    size2=len(string2)
    temp=""
    if size1!=size2:
        return 0
    temp=string1+string2
    if(temp.count(string2)>0):
        return 1
    else:
        return 0
string1="abcde"
string2="cdefs"

if areRotations(string1,string2):
        print("string are rotations")
else:
        print("strings are not rotations")
        
output:string are rotations
